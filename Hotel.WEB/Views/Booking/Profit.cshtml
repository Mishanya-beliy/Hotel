@model Hotel.WEB.Models.ProfitModel

<div class="container">
    <h4>Profit</h4>
    <hr />
    <div class="row">
            <form asp-action="Profit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group col s12 m6">
                    <label asp-for="Start" class="control-label"></label>
                    <input asp-for="Start" class="form-control" />
                    <span asp-validation-for="Start" class="text-danger"></span>
                </div>
                <div class="form-group col s12 m6">
                    <label asp-for="End" class="control-label"></label>
                    <input asp-for="End" class="form-control" />
                </div><br />
                      <div class="form-group">
                          <label asp-for="Money" class="control-label"></label>
                          @Html.DisplayFor(model => model.Money)
                          <input type="submit" value="Calculate" class="btn right red btn-primary" />
                      </div>
            </form>
    </div>
</div>
